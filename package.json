{
  "name": "nypl-discovery",
  "version": "1.7.0",
  "description": "Isomorphic React App for NYPL Research Catalog.",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "prod-start": "NODE_ENV=production PLATFORM_API_BASE_URL=https://platform.nypl.org/api/v0.1 node index",
    "dev-api-start": "PLATFORM_API_BASE_URL=https://dev-platform.nypl.org/api/v0.1 KMS_ENV=unencrypted node index",
    "prod-api-start": "PLATFORM_API_BASE_URL=https://platform.nypl.org/api/v0.1 KMS_ENV=encrypted node index",
    "eb-start": "npm run dist && node index",
    "dist": "NODE_ENV=production webpack --config webpack.config.js",
    "preinstall": "npm i webpack-cli",
    "postinstall": "npm run dist",
    "test": ". ./test.env; node_modules/.bin/mocha test/helpers/browser.js test/unit/*.test.js",
    "test-dev": "node_modules/.bin/mocha -w test/helpers/browser.js test/unit/*.test.js",
    "test-file": "node_modules/.bin/mocha -w test/helpers/browser.js",
    "coverage": "BABEL_ENV=test node_modules/.bin/nyc --require mocha npm test",
    "coverage-report": "BABEL_ENV=test node_modules/.bin/nyc report --reporter=lcov",
    "nightwatch": "./node_modules/.bin/nightwatch",
    "deploy-development": "eb deploy discovery-ui-development --profile nypl-sandbox",
    "deploy-qa": "git checkout qa && git pull origin qa && eb deploy discovery-ui-qa --profile nypl-digital-dev",
    "deploy-production": "git checkout master && git pull origin master && eb deploy discovery-ui-production --profile nypl-digital-dev",
    "map-names": "node -e 'require(\"./easyImports\").write()'"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "sass",
    "hot",
    "reload"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com//issues"
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "axios-mock-adapter": "1.7.1",
    "babel-core": "^7.0.0-bridge.0",
    "chai": "3.5.0",
    "create-react-class": "15.5.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "esdoc": "^1.1.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "7.5.1",
    "jsdom": "9.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "4.0.1",
