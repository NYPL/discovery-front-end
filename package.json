    "react-doc-meta": "0.2.0",
    "react-document-title": "2.0.3",
    "react-dom": "^16.12.0",
    "react-ga": "1.2.0",
    "react-hot-loader": "1.2.8",
    "react-number-format": "2.0.4",
    "react-onclickout": "2.0.6",
    "react-redux": "^7.2.1",
    "react-router": "3.0.5",
